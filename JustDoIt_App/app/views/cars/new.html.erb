<form action="/cars" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label>No. of Seat Available</label>
  <input type="text" name="seat_number">

  <label>Meeting Point</label>
  <input type="text" name="meeting_point">

  <label>Suburb</label>
  <select name="suburb">
    <option value=""></option>
    <option value=""></option>
  </select>

  <label>Smoker Friendly</label>
  <select name="smoker_friendly">
    <option value="no">No</option>
    <option value="yes">Yes</option>
  </select>

  <label>date</label>
  <input type="date" name="date" max="2017-10-15" min="2017-10-09">

  <label>time</label>
  <input type="time" name="time">

  <label>Additional Details</label>
  <textarea name="description" rows="8" cols="80"></textarea>
  <button>Post</button>
</form>
<a href="/home">Cancel</a>
